{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "2captcha/2captcha": "*",
        "aws/aws-sdk-php": "^3.247",
        "barryvdh/laravel-snappy": "^1.0",
        "dcblogdev/laravel-microsoft-graph": "^3.2",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "helvetitec/lagoon-charts": "~2.0",
        "ilovepdf/ilovepdf-php": "^1.2",
        "intervention/image": "^2.7",
        "itsgoingd/clockwork": "^5.1",
        "laravel/framework": "10.*",
        "laravel/horizon": "^5.24",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.10",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "3.*",
        "log1x/laravel-webfonts": "^1.0",
        "microsoft/microsoft-graph": "^1.85",
        "nesbot/carbon": "^2.66",
        "nyholm/psr7": "*",
        "opcodesio/log-viewer": "^3.1",
        "php-webdriver/webdriver": "^1.14",
        "railsware/mailtrap-php": "*",
        "regulus/tetra-text": "^0.6.16",
        "rmunate/spell-number": "^4.1",
        "setasign/fpdf": "1.8.*",
        "setasign/fpdi": "^2.0",
        "spatie/browsershot": "^3.61",
        "spatie/image": "^2.2",
        "spatie/simple-excel": "^3.6",
        "symfony/http-client": "*",
        "symfony/mailgun-mailer": "^6.2",
        "symfony/panther": "^2.0",
        "twilio/sdk": "^6.44",
        "typesense/typesense-php": "^4.9",
        "verumconsilium/laravel-browsershot": "^1.3",
        "vlucas/phpdotenv": "^5.5",
        "weidner/goutte": "^2.2",
        "zoon/puphpeteer": "^2.1"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.8",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pre-autoload-dump": [

        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
